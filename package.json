{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "playwright test --config=e2e/config/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=e2e/config/playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config=e2e/config/playwright.config.ts --headed",
    "db:check": "tsx scripts/check-database-sync.ts",
    "db:migrate": "tsx scripts/run-migrations.ts",
    "db:connect": "tsx scripts/connect-and-update-db.ts"
  },
  "dependencies": {
    "@sentry/react": "^10.34.0",
    "@supabase/supabase-js": "^2.57.4",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.3.0",
    "xlsx": "^0.18.5",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.9",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.17",
    "autoprefixer": "^10.4.18",
    "axe-playwright": "^2.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^4.0.17"
  }
}
